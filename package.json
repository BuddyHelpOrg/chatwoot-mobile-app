{
  "name": "@chatwoot/mobile-app",
  "version": "4.0.16",
  "main": "expo/AppEntry.js",
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "start": "expo start",
    "start:production": "expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "watchman watch-del-all && rm -rf node_modules ios android .expo && pnpm store prune && rm pnpm-lock.yaml",
    "reset": "pnpm clean && pnpm install && pnpm generate",
    "test": "jest",
    "e2e": "node e2e/check-environment.js",
    "lint": "eslint .",
    "generate": "npx expo prebuild --clean",
    "generate:soft": "npx expo prebuild",
    "run:ios": "npx expo run:ios",
    "run:android": "npx expo run:android -d",
    "build:android:local": "eas build -p android --profile production --local",
    "build:android": "eas build -p android --profile production",
    "build:ios:local": "eas build -p ios --profile preview --local --non-interactive",
    "build:ios": "eas build -p ios --profile production",
    "build:all:local": "eas build -p ios --profile production --local && eas build -p android --profile production --local",
    "build:all": "eas build -p ios --profile production && eas build -p android --profile production",
    "submit:android": "dotenv -c -- eas submit -p android --profile production",
    "submit:ios": "dotenv -e .env -- eas submit --platform ios",
    "submit:all": "dotenv -e .env -- eas submit -p ios --profile production && dotenv -e .env -- eas submit -p android --profile production",
    "storybook-generate": "sb-rn-get-stories",
    "start:storybook": "cross-env EXPO_STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env EXPO_STORYBOOK_ENABLED='true' pnpm run:ios",
    "storybook:android": "cross-env EXPO_STORYBOOK_ENABLED='true' expo android",
    "adb:connect": "adb reverse tcp:9090 tcp:9090",
    "appium": "node e2e/start-appium.js",
    "test:e2e:ios": "NODE_OPTIONS=\"--experimental-vm-modules\" node e2e/ios/specs/login.test.js",
    "test:e2e:android": "NODE_OPTIONS=\"--experimental-vm-modules\" node e2e/android/specs/login.test.js",
    "test:e2e:ios:timeout": "NODE_OPTIONS=\"--experimental-vm-modules\" node e2e/run-with-timeout.js 120 e2e/ios/specs/login.test.js",
    "test:e2e:android:timeout": "NODE_OPTIONS=\"--experimental-vm-modules\" node e2e/run-with-timeout.js 120 e2e/android/specs/login.test.js",
    "e2e:timeout": "pnpm test:e2e:ios:timeout && pnpm test:e2e:android:timeout",
    "e2e:safe": "pnpm appium & (sleep 5 && pnpm e2e:timeout); kill %1",
    "e2e:setup": "pnpm appium & sleep 5",
    "e2e:android": "emulator -avd Pixel_2_API_35 -no-window -no-audio -no-boot-anim & sleep 15 && pnpm test:e2e:android",
    "e2e:full": "pnpm e2e:setup && pnpm e2e",
    "e2e:run": "./e2e/run-tests.sh",
    "e2e:quick": "./e2e/quick-test.sh"
  },
  "dependencies": {
    "@alantoa/lightbox": "0.3.1",
    "@chatwoot/markdown-to-txt": "^2.0.4",
    "@chatwoot/react-native-widget": "^0.0.21",
    "@chatwoot/utils": "^0.0.42",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@kesha-antonov/react-native-action-cable": "^1.1.5",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^21.14.0",
    "@react-native-firebase/messaging": "^21.14.0",
    "@react-native-menu/menu": "^1.2.3",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/react-native": "^6.10.0",
    "@shopify/flash-list": "^1.8.0",
    "@types/lodash": "^4.17.16",
    "axios": "^1.8.4",
    "camelcase": "^8.0.0",
    "camelcase-keys": "^7.0.2",
    "cross-env": "^7.0.3",
    "date-fns": "2.21.1",
    "diff": "5.0.0",
    "expo": "~52.0.46",
    "expo-application": "~6.0.2",
    "expo-av": "~15.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "^17.0.8",
    "expo-document-picker": "^13.0.3",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.7",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "i18n-js": "^3.9.2",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-hook-form": "^7.55.0",
    "react-native": "0.76.7",
    "react-native-audio-recorder-player": "^3.6.12",
    "react-native-autoheight-webview": "^1.6.5",
    "react-native-device-info": "^11.1.0",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-image-picker": "^7.2.3",
    "react-native-ios-context-menu": "^3.1.1",
    "react-native-ios-utilities": "^5.1.4",
    "react-native-keyboard-controller": "^1.17.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-pager-view": "^6.7.0",
    "react-native-permissions": "^5.3.0",
    "react-native-reanimated": "^3.17.3",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "^4.10.0",
    "react-native-snackbar": "^2.9.0",
    "react-native-svg": "^15.11.2",
    "react-native-webview": "^13.13.5",
    "react-redux": "^9.2.0",
    "reactotron-redux": "^3.2.0",
    "redux-persist": "^6.0.0",
    "rn-fetch-blob": "^0.12.0",
    "semver": "^7.7.1",
    "snakecase-keys": "^8.0.1",
    "string.prototype.matchall": "4.0.12",
    "tailwindcss": "^3.4.17",
    "twrnc": "^4.6.1",
    "use-deep-compare-effect": "^1.8.1",
    "zeego": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-community/slider": "^4.5.6",
    "@storybook/addon-ondevice-actions": "^8.6.2",
    "@storybook/addon-ondevice-controls": "^8.6.2",
    "@storybook/react-native": "^8.6.2",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.20",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "appium": "^2.17.1",
    "appium-uiautomator2-driver": "^4.2.0",
    "appium-xcuitest-driver": "^9.2.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "react-dom": "18.3.1",
    "reactotron-react-native": "^5.1.12",
    "typescript": "^5.8.3",
    "wd": "^1.14.0",
    "webdriverio": "^8.44.1"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "@kesha-antonov/react-native-action-cable"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@kesha-antonov/react-native-action-cable",
          "react-native-audio-recorder-player",
          "react-native-file-viewer",
          "react-native-fs",
          "react-native-snackbar",
          "@react-native-community/blur",
          "react-native-autoheight-webview",
          "redux-persist",
          "rn-fetch-blob",
          "@alantoa/lightbox",
          "@chatwoot/markdown-to-txt",
          "@chatwoot/react-native-widget",
          "@chatwoot/utils",
          "camelcase",
          "camelcase-keys",
          "cross-env",
          "date-fns",
          "lodash",
          "reactotron-redux",
          "tailwindcss",
          "use-deep-compare-effect",
          "diff",
          "string.prototype.matchall"
        ],
        "listUnknownPackages": true
      }
    }
  }
}
